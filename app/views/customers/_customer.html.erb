<div id="<%= dom_id customer %>">
 
  <p>
    <strong>Full name:</strong>
    <%= customer.full_name %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= customer.phone %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= customer.email %>
  </p>

  <p>
    <strong>Number of Orders</strong>
    <%= customer.orders.count %>
  </p>


</div>
